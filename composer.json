{
  "name": "astonishing-endurance/flutter-backend",
  "description": "API Backend for Flutter Application",
  "type": "project",
  "require": {
    "php": "^8.1",
    "php-di/php-di": "^7.0",
    "slim/slim": "^4.0",
    "php-di/slim-bridge": "^3.4.1",
    "slim/psr7": "^1.3.0",
    "monolog/monolog": "^3.0",
    "twig/twig": "^3.0",
    "slim/twig-view": "^3.0"
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "optimize-autoloader": true
  },
  "scripts": {
    "start": "php -S 0.0.0.0:$PORT -t public"
  }
}
